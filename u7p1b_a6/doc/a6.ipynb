    "import math\n",
    "def standarization(X):\n",
    "    ave=np.sum(X,axis=0)\n",
    "#     print(ave)\n",
    "    n=X.shape[0]\n",
    "    normData = X - np.tile(ave,(n,1))\n",
    "#     print(normData.shape)\n",
    "    mins=X.min(0)\n",
    "    maxs=X.max(0)\n",
    "    diff=maxs-mins\n",
    "    print(diff)\n",
    "    normData = normData / np.tile(diff,(n,1))\n",
    "    return normData\n",
