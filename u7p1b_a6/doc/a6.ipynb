    "# q2\n",
    "y=X[:,-1]\n",
    "X=X[:,1:24]\n",
    "print(X[0,0])\n",
    "delay=X[:,6:12]\n",
    "num_delay=[]\n",
    "num_delay.append(delay[delay<1].size)\n",
    "for i in range(1,9):\n",
    "    num_delay.append(delay[delay==i].size)\n",
